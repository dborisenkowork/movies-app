{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "movies"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "modules/MoviesApp/assets",
        "modules/MoviesApp/assets/favicon/ico_16x16.ico"
      ],
      "index": "./modules/MoviesApp/entry-point/index.html",
      "main": "./modules/MoviesApp/entry-point/main.ts",
      "polyfills": "./modules/MoviesApp/entry-point/polyfills.ts",
      "test": "./modules/MoviesApp/entry-point/test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "movies",
      "styles": [
        "modules/MoviesApp/assets/global-styles/index.less"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.config.ts",
      "environments": {
        "dev": "environments/environment.config.ts",
        "mock": "environments/environment-mock.config.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "less",
    "component": {}
  }
}
